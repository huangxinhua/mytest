
import java.io.Serializable;

public class TableVO implements Serializable {

    private static final long serialVersionUID = -5448400926021626449L;
    private String primaryKeyField; // 表的主键名称,转java字段
    private String tableColumName; // 表中列名称
    private String columnDataType; // 表中列的类型
    private String columnComments; // 表中列的备注
    private String fieldName; // java中vo属性名称
    private String upperFieldName; // java中vo属性名称
    private String xmlFieldDataType; //dao.xml中列的数据类型, 通过表字段类型转maybatis数据类型保存
    private String fieldDataType; //vo中字段的类型, 通过表字段类型转java基本类型保存
    private Long columnId;
    
    public Long getColumnId() {
    
        return columnId;
    }

    public void setColumnId(Long columnId) {
    
        this.columnId = columnId;
    }

    public String getUpperFieldName() {
    
        return upperFieldName;
    }

    public void setUpperFieldName(String upperFieldName) {
    
        this.upperFieldName = upperFieldName;
    }

    public String getXmlFieldDataType() {
    
        return xmlFieldDataType;
    }

    public void setXmlFieldDataType(String xmlFieldDataType) {
    
        this.xmlFieldDataType = xmlFieldDataType;
    }

    public String getFieldDataType() {
    
        return fieldDataType;
    }

    public void setFieldDataType(String fieldDataType) {
    
        this.fieldDataType = fieldDataType;
    }

    public String getFieldName() {
    
        return fieldName;
    }

    public void setFieldName(String fieldName) {
    
        this.fieldName = fieldName;
    }

    public String getPrimaryKeyField() {

        return primaryKeyField;
    }

    public void setPrimaryKeyField(String primaryKeyField) {

        this.primaryKeyField = primaryKeyField;
    }

    public String getTableColumName() {

        return tableColumName;
    }

    public void setTableColumName(String tableColumName) {

        this.tableColumName = tableColumName;
    }

    public String getColumnDataType() {

        return columnDataType;
    }

    public void setColumnDataType(String columnDataType) {

        this.columnDataType = columnDataType;
    }

    public String getColumnComments() {

        return columnComments;
    }

    public void setColumnComments(String columnComments) {

        this.columnComments = columnComments;
    }

}
